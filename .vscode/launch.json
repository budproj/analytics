{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Analytics",
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env.local",
      "runtimeExecutable": "npm",
      "port": 9229,
      "timeout": 15000,
      "restart": true,
      "runtimeArgs": ["run-script", "start:debug"],
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
