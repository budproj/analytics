    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
