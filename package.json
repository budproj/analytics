{
  "name": "@budproj/analytics",
  "main": "src/main.js",
  "version": "1.0.0",
  "description": "Application to enable the evaluation of our data",
  "license": "Apache-2.0",
  "author": "delucca <delucca@pm.me>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budproj/analytics.git"
  },
  "bugs": {
    "url": "https://github.com/budproj/analytics/issues"
  },
  "engines": {
    "npm": ">=7.20.3",
    "node": ">=16.6.2"
  },
  "scripts": {
    "prepare": "husky install",
    "prebuild": "npm run clean",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "compile": "tsc",
    "clean": "rimraf -rf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "orm": "run-s clean compile && npm run orm:run --",
    "orm:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config src/config/orm/orm-cli.config",
    "orm:migrate": "npm run orm migration:run",
    "orm:migration-revert": "npm run orm migration:revert",
    "orm:migration-format": "prettier --write 'src/infrastructure/orm/migrations/*.ts'"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@grpc/proto-loader": "^0.6.4",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "8.0.6",
    "@nestjs/microservices": "8.0.6",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "nest-winston": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@budproj/eslint-config-base-ts": "^1.0.5",
    "@budproj/eslint-config-prettier-ts": "^1.0.0",
    "@budproj/prettier-config": "^1.0.0",
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.2",
    "@types/express": "4.17.13",
    "@types/node": "14.17.9",
    "@types/ws": "7.4.7",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^28.0.0",
    "eslint-plugin-unused-imports": "^1.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
