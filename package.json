    "@nestjs/platform-fastify": "7.6.18",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "nest-winston": "^1.4.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@budproj/eslint-config-base-ts": "^1.0.5",
